<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.homework.egms.mapper.IncidentMapper">

   <select id="findIncident" parameterType="java.lang.Long" resultType="com.homework.egms.bean.Incident">
       SELECT * FROM incidents WHERE incident_id=#{incidentId}
   </select>

</mapper>